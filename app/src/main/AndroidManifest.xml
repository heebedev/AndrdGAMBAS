<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pjt.andrdgambas">
    <!-- // 인터넷 사용 권한 추가 : 송희 -->
    <!-- 인터넷을 타고 나갈려면 필요하다. ******************************************* -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".LOGIN.GlobalApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/GAMBASTheme"
        android:usesCleartextTraffic="true">

        <activity android:name=".MYINFO.ChangePasswordActivity"/>
        <activity android:name=".MYINFO.UpdateMyInfoActivity" />
        <activity android:name=".FIREBASE.TestContentsActivity" />
        <activity android:name=".FIREBASE.AddContentsActivity" />

        <activity android:name=".MYCHANNEL.Activity_Mychannel"></activity>
        <activity android:name=".LOGIN.FindIdPwActivity"></activity>
        <activity android:name=".LOGIN.SignUpActivity" />

        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="@string/kakao_app_key" />
        <activity android:name=".LOGIN.EmailLoginActivity" />
        <activity android:name=".LOGIN.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".SUBSCRIBE.Activity_Subscribe_ContentsList"></activity>
        <activity android:name=".SUBSCRIBE.Activity_Subscribe_Contents_Detail"></activity>
        <activity android:name=".HOME.MainActivity">
        </activity>
    </application>

</manifest>